{
    "name": "zzzul/generator",
    "description": "Laravel starter app & simple crud generator",
    "type": "library",
    "license": "MIT",
    "keywords": ["package", "laravel", "starter-app", "crud-generator"],
    "autoload": {
        "psr-4": {
            "Zzzul\\Generator\\": "src/",
            "Tests\\": "tests/"
        },
        "files": [
            "src/Helpers/helper.php"
        ]
    },
    "authors": [
        {
            "name": "Mohammad Zulfahmi",
            "email": "mohammad.zulfahmi03@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "intervention/image": "^2.7",
        "laravel/fortify": "^1.13",
        "spatie/laravel-permission": "^5.5",
        "yajra/laravel-datatables-oracle": "^10.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "nunomaduro/collision": "^6.3",
        "orchestra/testbench": "^7.14"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "@php vendor/bin/testbench package:discover --ansi"
        ]
    }
}
